javascript:(function() {  var url = window.location.href;  var id = url.split("id=")[1];  var newTabUrl = "https://x.com/compose/post";  var newUrl = "https://website.com/articleid/" + id + "/";  var title = document.querySelector('input[name="ctl00$ContentPlaceHolder1$txtTitle"]').value;  window.open(newTabUrl + "?url=" + encodeURIComponent(title + "\n" + newUrl), "_blank");  })();
